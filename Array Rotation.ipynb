   "source": [
    "arr = [1,2,3,4,5,6,7,8]\n",
    "n = len(arr)\n",
    "def rotate(arr, n): \n",
    "    x = arr[n - 1] \n",
    "      \n",
    "    for i in range(n - 1, 0, -1): \n",
    "        arr[i] = arr[i - 1]; \n",
    "          \n",
    "    arr[0] = x; \n",
    "    \n",
    "rotate(arr, n) \n",
    "  \n",
    "print (\"Rotated array is\") \n",
    "for i in range(0, n): \n",
    "    print (arr[i], end = ' ') "
   ]
