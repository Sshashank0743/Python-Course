    "def check(N, M):\n",
    " \n",
    "    # exceptional case for F(2)\n",
    "    if (N == 9 or M == 2 or\n",
    "        N % M == 0) :\n",
    "        print( \"No\" )\n",
    "     \n",
    "    # if none of the above\n",
    "    # cases, hence not divisible\n",
    "    else :\n",
    "        print( \"Yes\" )\n",
    " \n",
    "# Driver Code\n",
    "M = 2\n",
    "N = 9\n",
    "check(N, M)"
